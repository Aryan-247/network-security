#include <stdio.h>
 #include <conio.h>
 int main() { 
    char password[10], username[10], ch; 
    int i; 
    printf("Enter Username: ");     gets(username);     printf("Enter Password: "); 
    for(i = 0; i < 8; i++) {  
       ch = getch();     
     password[i] = ch;    
     printf("*");   
    } 
    //password[i] = '\0';  // Null terminate the string    
 printf("\nYour password is: ");   
  for(i = 0; i < 8; i++) { 
        printf("%c", password[i]);  
// Print the actual password 
    } 
    getch(); 
return 0; 
} 
